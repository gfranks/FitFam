<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EndSheetBehavior">
        <attr name="esb_peekWidth" format="dimension" />
        <attr name="esb_hideable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WCRecyclerView">
        <attr name="wcrv_layoutAnimation" format="boolean" />
        <attr name="wcrv_dividers" format="boolean" />
        <attr name="wcrv_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SeekArc">
        <attr name="sa_thumb" format="reference" />
        <attr name="sa_thumbOffset" format="dimension" />
        <attr name="sa_max" format="integer" />
        <attr name="sa_progressWidth" format="dimension" />
        <attr name="sa_arcWidth" format="dimension" />
        <attr name="sa_progress" format="integer" />
        <attr name="sa_rotation" format="integer" />
        <attr name="sa_startAngle" format="integer" />
        <attr name="sa_sweepAngle" format="integer" />
        <attr name="sa_arcColor" format="color" />
        <attr name="sa_progressColor" format="color" />
        <attr name="sa_roundEdges" format="boolean" />
        <attr name="sa_touchInside" format="boolean" />
        <attr name="sa_clockwise" format="boolean" />
        <attr name="sa_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SeekArcTheme">
        <attr name="seekArcStyle" format="reference" />
    </declare-styleable>

</resources>